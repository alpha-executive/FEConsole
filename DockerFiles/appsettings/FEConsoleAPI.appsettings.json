{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:8091"
      }
    }
  },  
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "sqliteDb": "Data Source=/app/db/ferepository.db",
    "storedfileconnstr": "Data Source=/app/db/storedfile.db"
  },
  "SiteSettings": {
    "IsDataForLoginUserOnly": true,
    "databaseprovider": "sqlite",
    "SuperAdmin": "admin@fetechlab.com",
    "ForceHttps": false,
    "Cache": {
      "CacheSize": 100,
      "AbsoluteExpiration": 30
    },
    "CORS": [ "http://feconsole_feadmin:8090" ],
    "Security": {
      "PublicKey": "BgIAAACkAABSU0ExAAQAAAEAAQBPkHSfs7Ukfng9Dz4EZZ1bDw5wCo4zKglQDlzOx01/b69bvLqxg2COkfKpegMJH8uDSGd8fvSSBKoWFu1RGnTomNUMHB7FRrbDAYQ0VAyUNfUcrZps8YlqgAjFGt3pF5GSoT7vGVVt3dKaRinvcPmlF3mk9qM/DHtqPfp4oA2Hqw==",
      "PrivateKey": ""
    },
    "FileUpload": {
      "FileSizeLimit": 268435456,
      "PermittedExtension": [ ".txt", ".jpg", ".png", ".bmp", ".jpeg", ".doc", ".docx", ".pdf", ".xls", ".xlsx", ".ppt", ".pptx", ".zip", ".7z", ".tar", ".gz" ]
    },
    "FileStorageService": {
      "Thumbinal": {
        "Width": 260,
        "Height": 260
      }
    },
    "FileStoreProviders": {
      "FileSystem": {
        "RootDir": "appdata"
      }
    },
    "ReverseProxy": {
      "Enabled": false,
      "AllowedIPAddress": ["node1"]
    }
  },
  "Authentication": {
    "IdentityServer": {
      "Url": "http://feconsole_feidentity:8092",
      "RequireHttpsMetadata": false
    }
  }
}
